@model Student

<div class="card" style="width:400px">
    <div class="card-body">
        <h4 class="card-title">@Model.Name</h4>
        <p class="card-text">@Model.Address</p>
        <a asp-action="Details" asp-controller="Student" asp-route-id="@Model.ID" class="btn btn-primary">See Profile</a>
    </div>
    <img class="card-img-bottom" src="/images/@Model.Img" alt="Card image" style="width:100%">
</div>

